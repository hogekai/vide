{
  "name": "@videts/vide",
  "version": "0.9.7",
  "description": "Modular, headless video player. Ads, streaming, DRM â€” use only what you need. Core under 3KB gzip.",
  "type": "module",
  "unpkg": "dist/vide.global.js",
  "jsdelivr": "dist/vide.global.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./vast": {
      "types": "./dist/vast/index.d.ts",
      "import": "./dist/vast/index.mjs"
    },
    "./vmap": {
      "types": "./dist/vmap/index.d.ts",
      "import": "./dist/vmap/index.mjs"
    },
    "./omid": {
      "types": "./dist/omid/index.d.ts",
      "import": "./dist/omid/index.mjs"
    },
    "./simid": {
      "types": "./dist/simid/index.d.ts",
      "import": "./dist/simid/index.mjs"
    },
    "./hls": {
      "types": "./dist/hls/index.d.ts",
      "import": "./dist/hls/index.mjs"
    },
    "./dash": {
      "types": "./dist/dash/index.d.ts",
      "import": "./dist/dash/index.mjs"
    },
    "./drm": {
      "types": "./dist/drm/index.d.ts",
      "import": "./dist/drm/index.mjs"
    },
    "./ssai": {
      "types": "./dist/ssai/index.d.ts",
      "import": "./dist/ssai/index.mjs"
    },
    "./ui": {
      "types": "./dist/ui/index.d.ts",
      "import": "./dist/ui/index.mjs"
    },
    "./ui/theme.css": "./dist/ui/theme.css",
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs"
    },
    "./vue": {
      "types": "./dist/vue/index.d.ts",
      "import": "./dist/vue/index.mjs"
    },
    "./svelte": {
      "types": "./dist/svelte/index.d.ts",
      "svelte": "./dist/svelte/index.js",
      "default": "./dist/svelte/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup && pnpm build:svelte",
    "build:svelte": "svelte-package -i src/svelte -o dist/svelte && find src -name '*.d.ts' -o -name '*.d.ts.map' | xargs rm -f",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:install": "playwright install --with-deps",
    "lint": "biome check src tests",
    "lint:fix": "biome check --write src tests",
    "typecheck": "tsc --noEmit",
    "docs:api": "typedoc",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@playwright/test": "^1.58.2",
    "@testing-library/react": "^16.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/svelte": "^5.0.0",
    "@vue/test-utils": "^2.4.6",
    "dashjs": "^4.7.0",
    "hls.js": "^1.6.15",
    "jsdom": "^25.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.0.0",
    "typedoc": "^0.28.17",
    "typedoc-plugin-markdown": "^4.10.0",
    "typescript": "^5.6.0",
    "vitepress": "^1.6.4",
    "vitest": "^2.0.0",
    "svelte": "^5.0.0",
    "vue": "^3.5.29"
  },
  "peerDependencies": {
    "dashjs": "^4.0.0",
    "hls.js": "^1.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "svelte": "^5.0.0",
    "vue": "^3.3.0"
  },
  "peerDependenciesMeta": {
    "dashjs": {
      "optional": true
    },
    "hls.js": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "svelte": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hogekai/vide"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@10.30.1",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@biomejs/biome",
      "esbuild"
    ]
  }
}
